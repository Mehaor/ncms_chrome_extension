{
  "manifest_version": 2,

  "name": "Ncms Site Extension",
  "description": "Extension for retrieving DOM sequences from sources for ncms scanner",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

